{
    "name": "mblsolutions/paypoint-sendgrid-notification",
    "description": "PayPoint Sendgrid Notification package",
    "type": "package",
    "license": "proprietary",
    "authors": [
        {
            "name": "MBL Solutions Ltd",
            "email": "tech@mblsolutions.co.uk"
        }
    ],
    "require": {
        "php": "^8.1|8.2|8.3|8.4",
        "ext-json": "*",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/events": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/mail": "^10.0|^11.0|^12.0",
        "symfony/http-client": "^6.0|^7.0",
        "symfony/sendgrid-mailer": "^6.0|^7.0",
        "lerouse/laravel-repository": "^2.6",
        "guzzlehttp/guzzle": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "orchestra/testbench": "^8.0||^9.0||^10.0||^11.0",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^10.0||^11.0||^12.0"
    },
    "autoload": {
        "psr-4": {
            "MBLSolutions\\SendgridNotification\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MBLSolutions\\SendgridNotification\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "MBLSolutions\\SendgridNotification\\SendgridNotificationServiceProvider"
            ]
        }
    }
}
